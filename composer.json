{
    "name": "zndron/nova-tabs",
    "description": "Laravel Nova - Tabs.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0",
        "nova-kit/nova-packages-tool": "^1.3",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "orchestra/testbench": "^6",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Zndron\\Tabs\\": "src/",
            "Zndron\\Tabs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zndron\\Tabs\\TabsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix",
        "phpunit": "vendor/bin/phpunit",
        "test": [
            "composer php-cs-fixer",
            "composer phpunit"
        ]
    }
}
